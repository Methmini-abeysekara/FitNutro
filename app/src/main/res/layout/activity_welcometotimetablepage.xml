<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/pic"
    android:textAlignment="center"
    tools:context=".WelcomeToTimetablePage"
    android:gravity="center_horizontal">

    <Button
        android:id="@+id/tstartedbtn"
        android:layout_width="141dp"
        android:layout_height="63dp"
        android:layout_marginTop="92dp"
        android:backgroundTint="#00416A"
        android:text="@string/tstartedbtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/maintext1" />

    <TextView
        android:id="@+id/maintext1"
        android:layout_width="333dp"
        android:layout_height="81dp"
        android:layout_marginTop="84dp"
        android:fontFamily="@font/secular_one"
        android:text="@string/maintext1"
        android:textAlignment="center"
        android:textColor="#095E63"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:gravity="center_horizontal" />

    <Button
        android:id="@+id/wcalbtn"
        android:layout_width="356dp"
        android:layout_height="63dp"
        android:layout_marginBottom="90dp"
        android:backgroundTint="#00416A"
        android:text="@string/wcalbtn"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tstartedbtn"
        app:layout_constraintVertical_bias="1.0" />

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginBottom="24dp"
        android:background="@drawable/cus_btn2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/ic_baseline_home_24"
        tools:ignore="VectorDrawableCompat" />


</androidx.constraintlayout.widget.ConstraintLayout>