<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@drawable/pic"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Adddetails">

    <TextView
        android:id="@+id/listex3"
        android:layout_width="78dp"
        android:layout_height="30dp"
        android:layout_marginTop="500dp"
        android:fontFamily="@font/alef"
        android:text="@string/listex3"
        android:textAlignment="viewStart"
        android:textColor="#000000"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.279"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:gravity="start" />

    <TextView
        android:id="@+id/time5"
        android:layout_width="352dp"
        android:layout_height="42dp"
        android:layout_marginTop="404dp"
        android:fontFamily="@font/concert_one"
        android:text="@string/time5"
        android:textAlignment="viewStart"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:gravity="start" />

    <EditText
        android:id="@+id/txtlist"
        android:layout_width="354dp"
        android:layout_height="44dp"
        android:layout_marginTop="328dp"
        android:backgroundTint="#000000"
        android:ems="10"
        android:fontFamily="@font/concert_one"
        android:inputType="textPersonName"
        android:outlineProvider="none"
        android:text="@string/txtlist"
        android:textColor="#BC000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.561"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/time7"
        android:layout_width="107dp"
        android:layout_height="55dp"
        android:layout_marginTop="476dp"
        android:backgroundTint="#000000"
        android:ems="10"
        android:fontFamily="@font/concert_one"
        android:inputType="number"
        android:outlineProvider="none"
        android:textSize="18sp"
        android:text="@string/txtlist"
        android:textColor="#BC000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.609"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/add"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="64dp"
        android:backgroundTint="#00416A"
        android:onClick="showAlertDialog"
        android:text="@string/add"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.47"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="OnClick" />

    <TextView
        android:id="@+id/day"
        android:layout_width="352dp"
        android:layout_height="42dp"
        android:layout_marginTop="88dp"
        android:fontFamily="@font/concert_one"
        android:text="@string/day"
        android:textAlignment="viewStart"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.508"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:gravity="start" />

    <TextView
        android:id="@+id/list6"
        android:layout_width="352dp"
        android:layout_height="42dp"
        android:layout_marginTop="212dp"
        android:fontFamily="@font/concert_one"
        android:text="@string/list6"
        android:textAlignment="viewStart"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.542"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:gravity="start" />

    <TextView
        android:id="@+id/listex"
        android:layout_width="352dp"
        android:layout_height="42dp"
        android:layout_marginTop="268dp"
        android:fontFamily="@font/alef"
        android:text="@string/listex"
        android:textAlignment="viewStart"
        android:textColor="#000000"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.508"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:gravity="start" />

    <EditText
        android:id="@+id/day1"
        android:layout_width="354dp"
        android:layout_height="44dp"
        android:layout_marginTop="136dp"
        android:backgroundTint="#000000"
        android:ems="10"
        android:fontFamily="@font/concert_one"
        android:inputType="textPersonName"
        android:text="@string/day1"
        android:textColor="#BC000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.526"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="167dp"
        android:layout_height="185dp"
        android:layout_marginBottom="4dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.987"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/pic2" />


</androidx.constraintlayout.widget.ConstraintLayout>