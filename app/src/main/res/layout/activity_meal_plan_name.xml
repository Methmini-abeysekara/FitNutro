<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Meal_Plan_Main"
    android:background="@drawable/background"

    >

    <!--    <include-->
    <!--        android:id="@+id/include"-->
    <!--        layout="@layout/toolbar"-->
    <!--        tools:layout_editor_absoluteX="-172dp"-->
    <!--        tools:layout_editor_absoluteY="16dp" />-->

    <Button
        android:id="@+id/m_button"
        android:layout_width="0dp"
        android:layout_height="wrap_content"

        android:text="@string/started_button_meal_plan"
        app:backgroundTint="#50C5B7"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.306" />

    <Button
        android:id="@+id/m_button4"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/fatCalculator"
        app:backgroundTint="#50C5B7"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.688" />

    <Button
        android:id="@+id/m_button5"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/proteinCalculator"
        app:backgroundTint="#50C5B7"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/m_button4"
        app:layout_constraintVertical_bias="0.138"
        />

    <TextView
        android:id="@+id/m_textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/AreYoLookingFor"
        android:textColor="#50C5B7"
        android:textColorHint="#FFFFFF"
        android:textSize="23sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.158" />

    <TextView
        android:id="@+id/m_textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/MealPlansToCustomize"
        android:textColor="#50C5B7"
        android:textSize="23sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/m_textView2"
        app:layout_constraintVertical_bias="0.029" />


</androidx.constraintlayout.widget.ConstraintLayout>