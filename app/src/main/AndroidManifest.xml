<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fitnutro">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FitNutro">

        <activity android:name=".UpdateTask"
            android:label="Update Your Task"
            android:parentActivityName=".G_ScheduleList">

        </activity>

        <activity android:name=".StartTarget"
            android:label="Select Your Path">

        </activity>

        <activity android:name=".GainActivity"
            android:label="Weight Gain Workouts"
            android:parentActivityName=".G_ScheduleList">
        </activity>

        <activity android:name=".G_ScheduleList"
            android:label="Add Your Task"
            android:parentActivityName=".StartTarget">

        </activity>
        <activity android:name=".AddTask"
            android:label="Set Your Task"
            android:parentActivityName=".G_ScheduleList">


        </activity>
        <activity
            android:name=".WelcomePage"
            android:label="WELCOME TO FITNUTRO"
            android:parentActivityName=".StartFitnutro" />
        <activity
            android:name=".StartFitnutro"
            android:label="FITNUTRO">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>